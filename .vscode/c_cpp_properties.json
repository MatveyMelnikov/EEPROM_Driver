{
    "configurations": [
        {
            "name": "EEPROM_Driver",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/External/**",
                "${workspaceFolder}/Tests",
                "${workspaceFolder}/External/eeprom_driver/Inc",
                "${workspaceFolder}/Tests/mocks/Inc",
                "${workspaceFolder}/Core/Inc",
                "${workspaceFolder}/Drivers/STM32F1xx_HAL_Driver/Inc",
                "${workspaceFolder}/Drivers/STM32F1xx_HAL_Driver/Inc/Legacy",
                "${workspaceFolder}/Drivers/CMSIS/Device/ST/STM32F1xx/Include",
                "${workspaceFolder}/Drivers/CMSIS/Include"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "USE_HAL_DRIVER",
                "STM32F103xB"
            ],
            "intelliSenseMode": "gcc-arm",
            "compilerPath": "/usr/local/src/arm-gnu-toolchain-13.2/bin/arm-none-eabi-gcc",
            "compilerArgs": [
                "-mcpu=cortex-m3",
                "-mthumb"
            ],
            "cStandard": "c99",
            "cppStandard": "gnu++11"
        }
    ],
    "version": 4
}